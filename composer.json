{
  "name": "kuzyt/halfdream",
  "description": "Basic Laravel sites generator.",
  "keywords": ["laravel"],
  "license": "MIT",
  "homepage": "https://www.halfdream.ru/",
  "support": {
    "issues": "https://github.com/kuzyt/halfdream/issues",
    "source": "https://github.com/kuzyt/halfdream"
  },
  "authors": [
    {
      "name": "KuzyT",
      "email": "admin@kuzyt.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "KuzyT\\Halfdream\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KuzyT\\Halfdream\\Tests\\": "tests/"
    }
  },
  "require": {
    "laravel/framework": "^9.0",
    "intervention/image": "^2.4",
    "spatie/laravel-permission": "^5.5.0",
    "spatie/laravel-translatable": "^6.0.0",
    "codezero/laravel-unique-translation": "^3.5",
    "sokil/php-isocodes": "^4.1",
    "sokil/php-isocodes-db-i18n": "^4.0"
  },
  "require-dev": {

  },
  "extra": {
    "laravel": {
      "providers": [
        "KuzyT\\Halfdream\\HalfdreamServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
